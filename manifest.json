{
    "manifest_version": 2,
    "name": "Nirmit Social Media Launcher",
    "description": "Quick launch to Nirmit's social media",
    "version": "1.0.0",
    "icon": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon_19.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}